{
  "name": "cute-kitty-ui-cli",
  "version": "3.0.1",
  "description": "CLI tool for installing Cute-Kitty-UI components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/opcxder/cute-kitty-ui.git"
  },
  "bugs": {
    "url": "https://github.com/opcxder/cute-kitty-ui/issues"
  },
