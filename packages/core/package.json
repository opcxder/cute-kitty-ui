{
  "name": "cute-kitty-ui-core",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/opcxder/cute-kitty-ui.git"
  },
  "bugs": {
    "url": "https://github.com/opcxder/cute-kitty-ui/issues"
  },
  "homepage": "https://github.com/opcxder/cute-kitty-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --no-config",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --no-config",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.40.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "framer-motion": "^11.0.0"
  },
  "peerDependenciesMeta": {
    "framer-motion": {
      "optional": true
    }
  }
}
